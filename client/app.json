{
  "expo": {
    "name": "Laundry Booking",
    "scheme": "LaundryBooking",
    "newArchEnabled": true,
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/washing-machine.png",
      "resizeMode": "contain",
      "backgroundColor": "#363062"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/washing-machine.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-splash-screen"]
  }
}
